{
  "compilerOptions": {
    "module": "es2020",
    "target": "es2020",
    "sourceMap": true,
    "outDir": "build",
    "esModuleInterop": true,
    "allowJs": true,
    "baseUrl": ".",
    "allowImportingTsExtensions": true,
    "moduleResolution":"node",
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "noEmit": true,
  },
  "exclude": [
    "node_modules"
  ]
}